@using Day3.ViewModel
@model List<StudentsWithCoursesViewModel>

<table class="table table-dark">
        <thead>
    <tr>
            <th><h1>Student Name </h1></th>
            <th><h1>Course Name </h1></th>
    </tr>
    </thead>
    <tbody>
        
@foreach(var item in Model)
{
    <tr>
                <td> @item.StudentName.Name</td>
                <td> @item.CourseName.Name</td>
    </tr>
}
    </tbody>
</table>
<h4>Student Count: @TempData["Tempdata1"] </h4>
<h4>Courses Count: @TempData["Tempdata2"] </h4>

